{% block user %}
            <div id="content" class="p-4 p-md-5">

              <nav class="navbar navbar-expand-lg navbar-light bg-light">
                <div class="container-fluid">

                  {% block toogle %}
                      <button type="button" id="sidebarCollapse" class="btn deep-orange darken-4">
                        <i class="fa fa-bars"></i>
                        <span class="sr-only">Toggle Menu</span>
                      </button>
                      <button class="btn btn-dark d-inline-block d-lg-none ml-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                          <i class="fa fa-bars"></i>
                      </button>
                  {% endblock toogle %}

                  <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    

                  <ul class="nav navbar-nav ml-auto">
                      <li class="nav-item dropdown"> 
                          <a id="userIdMenu" class="nav-link dropdown-toggle" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                              {{ app.user }}
                          </a>
                          <ul class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                                      <li><a class="dropdown-item" href="{{ path('logout') }}">Cerrar Sesion</a></li>
                                      <li><a class="dropdown-item" href="{{ path('select_estructura') }}">Cambiar Estructura</a></li>              
                          </ul>   
                      </li>
                  </ul>

                  </div>
                </div>
              </nav>
               <div class="container">
                  {% block content %}
                          
                  {% endblock %}
              </div>
            </div>
{% endblock user %}